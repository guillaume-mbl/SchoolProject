<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>schoolProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>Test</servlet-name>
  	<servlet-class>com.guill.servlets.Test</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>AffichageClasses</servlet-name>
  	<servlet-class>com.guill.servlets.DisplayClassrooms</servlet-class>
  </servlet>
	<servlet>
		<servlet-name>AffichageBooks</servlet-name>
		<servlet-class>com.guill.servlets.DisplayBook</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AffichageMaterials</servlet-name>
		<servlet-class>com.guill.servlets.DisplayMaterial</servlet-class>
	</servlet>
  
  
  <servlet-mapping>
  	<servlet-name>Test</servlet-name>
  	<url-pattern>/toto</url-pattern>
  </servlet-mapping>
  
 
  <servlet-mapping>
  	<servlet-name>AffichageClasses</servlet-name>
  	<url-pattern>/classrooms</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>AffichageBooks</servlet-name>
  	<url-pattern>/books</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>AffichageMaterials</servlet-name>
  	<url-pattern>/materials</url-pattern>
  </servlet-mapping>
</web-app>